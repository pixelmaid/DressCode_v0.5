function butterfly()
	r1 = rotate(ellipse(95.00,120.00,444.00,212.00),20)
	r2 = moveTo(rotate(ellipse(269.00,201.00,100.00,262.00),45),208.0,276.0)
	r3 = moveTo(rotate(ellipse(88.00,-13.00,348.00,166.00),45),170.0,103.0)
	
	r4 = moveTo(ellipse(189.00,216.00,108.00,114.00),182.0,271.0)
	scale(r4,0.75,1.5)
	moveTo(r4,182.0,247.0)
	moveTo(rotate(r4,55),172.0,257.0)
	
	moveTo(scale(r4,1.2,1.2),155.0,270.0)
	scale(r2,1.2,1.2)
	moveTo(r2,184.0,301.0)
	
	bf = union(union(union(r1,r2),r3),r4)
	bf2 = copy(bf)
	bf2 = mirrorX(bf2)
	moveTo(bf2,488.0,getY(bf))
	
	bfly= union(bf,bf2)
	moveTo(bfly,WIDTH/2,HEIGHT/2)
	
	scale(bfly,0.75,0.75)
	return bfly
