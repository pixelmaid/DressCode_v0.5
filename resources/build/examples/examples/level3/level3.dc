//these are variables that control the pattern
myNum=5.50
slider(myNum,0,50)

myRadius=66
slider(myRadius,0,300)

myGroup= group()
radial myGroup, rad=myRadius, num=myNum:
	myShape()

moveTo(myGroup,165,201)
hide(myGroup)

myGap=198
myRowNum=5.60
myScale=0.42
slider(myGap,0,600);
slider(myRowNum,2,10);
slider(myScale,0.1,1);

myRow= group()
row myRow, gap=myGap, num=myRowNum:
	copy(myGroup)

moveTo(myRow,270,49)

scale(myRow,myScale)
